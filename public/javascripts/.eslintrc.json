{
    "extends": "standard",
    "plugins": [
        "standard"
    ],
    "rules": {
        "semi": ["error", "always"],
        "require-jsdoc": ["error", {
            "require": {
                "FunctionDeclaration": false,
                "MethodDefinition": false,
                "ClassDeclaration": false
            }
        }]
    },
    "env": {
        "browser": true,
        "jquery": true
    },
    "globals": {

    }
}
